<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.studentmanagementsystem.ResultDetailsController">
   <children>
      <Label fx:id="semesterLabel" layoutX="20.0" layoutY="20.0" text="Semester Result Details" textFill="#2d3447">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TableView fx:id="detailsTable" layoutX="20.0" layoutY="60.0" prefHeight="320.0" prefWidth="560.0">
        <columns>
          <TableColumn fx:id="colCourse" prefWidth="300.0" text="Course Name" />
          <TableColumn fx:id="colCredit" prefWidth="100.0" text="Credit" />
            <TableColumn fx:id="colGpa" prefWidth="100.0" text="GPA" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
